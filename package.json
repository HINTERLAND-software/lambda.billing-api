    "@types/node-fetch": "^2.5.7",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.2.1",
    "puppeteer": "^10.1.0",
